<block>
  <name>MIMO-OFDM Channel Estimator</name>
  <key>digital_mimo_ofdm_channel_estimator_vcvc</key>
  <category>[MIMO]</category>
  <import>from gnuradio import digital</import>
  <make>digital.mimo_ofdm_channel_estimator_vcvc($n, $n, $fft_len, $pilot_symbols, $pilot_carriers, $occupied_carriers, $csi_key, $start_key)</make>
  <param>
    <name>M</name>
    <key>m</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>N</name>
    <key>n</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>FFT Length</name>
    <key>fft_len</key>
    <value>64</value>
    <type>int</type>
  </param>
  <param>
    <name>Pilot Symbols</name>
    <key>pilot_symbols</key>
    <value>()</value>
    <type>raw</type>
  </param>
  <param>
    <name>Pilot Carriers</name>
    <key>pilot_carriers</key>
    <value>()</value>
    <type>int_vector</type>
  </param>
  <param>
    <name>Occupied Carriers</name>
    <key>occupied_carriers</key>
    <value>()</value>
    <type>int_vector</type>
  </param>
  <param>
    <name>CSI Tag Key</name>
    <key>csi_key</key>
    <value>"csi"</value>
    <type>string</type>
  </param>
  <param>
    <name>Start Tag Key</name>
    <key>start_key</key>
    <value>"start"</value>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$fft_len</vlen>
    <nports>$n</nports>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>len($occupied_carriers)</vlen>
    <nports>$n</nports>
  </source>
</block>
